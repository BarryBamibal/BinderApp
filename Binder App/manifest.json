{
  "manifest_version": 2,
  "name": "Binder App",
  "description": "Open web pages quickly from your desktop",
  "version": "1.2.1",
  
    "icons": {
    "16": "icon-supersmall.png",
	"48": "icon-small.png",
	"128": "icon-large.png"
  },

  "permissions": [
  "app.window",
  "storage"
  ],
  

   "app": {
      "background": {
         "scripts": [ "background.js" ]
      }
   }

}